<aura:component controller="Dynamic_Account_List">
    <aura:attribute name="accounts" type="Account[]" />
    <aura:handler name="init" value="{!this}" action="{!c.callMe}" />
    <aura:iteration items="{!v.accounts}" var="a">
    	<p>{!a.Name}----{!a.Phone}</p>
    </aura:iteration>
</aura:component>