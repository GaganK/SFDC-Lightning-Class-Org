<aura:component controller="App_Event_Account_Search">
    <aura:attribute name="acc" type="Account" />
    <aura:handler event="c:App_Event_Two" action="{!c.callMe}" />
    <div class="box">
        <lightning:input label="Account Name" value="{!v.acc.Name}" /> <br/>
        <lightning:input label="Account Phone" value="{!v.acc.Phone}" /><br/>
        <lightning:input label="Account Industry" value="{!v.acc.Industry}" />
    </div>
</aura:component>